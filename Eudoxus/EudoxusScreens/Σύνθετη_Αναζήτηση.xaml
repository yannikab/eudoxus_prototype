			<StackPanel Orientation="Horizontal" HorizontalAlignment="Left">

				<Button HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="5,0,0,0" Style="{StaticResource urlButton}" Content="Εύδοξος">
